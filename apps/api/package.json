{
  "name": "@acme/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/server.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "db:migrate": "tsx src/scripts/migrate.ts"
  },
  "dependencies": {
    "@acme/auth": "workspace:*",
    "@acme/users": "workspace:*",
    "@acme/billing": "workspace:*",
    "@acme/shared": "workspace:*",
    "@acme/config": "workspace:*",
    "@acme/db": "workspace:*",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0"
  }
}
